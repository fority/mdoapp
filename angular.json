                "inject": false
              },
              {
                "input": "src/themes/lara/lara-light-blue.css",
                "bundleName": "lara-blue",
                "inject": false
              },
              {
                "input": "src/themes/lara/lara-light-indigo.css",
                "bundleName": "lara-indigo",
                "inject": false
              },
              {
                "input": "src/themes/lara/lara-light-purple.css",
                "bundleName": "lara-purple",
                "inject": false
              },
              {
                "input": "src/themes/lara/lara-light-teal.css",
                "bundleName": "lara-teal",
                "inject": false
              },
              {
                "input": "src/themes/lara/lara-dark-blue.css",
                "bundleName": "lara-blue-dark",
                "inject": false
              },
              {
                "input": "src/themes/lara/lara-dark-indigo.css",
                "bundleName": "lara-indigo-dark",
                "inject": false
              },
              {
                "input": "src/themes/lara/lara-dark-purple.css",
                "bundleName": "lara-purple-dark",
                "inject": false
              },
              {
                "input": "src/themes/lara/lara-dark-teal.css",
                "bundleName": "lara-teal-dark",
                "inject": false
              },
              {
                "input": "src/themes/bootstrap/bootstrap4-dark-blue.css",
